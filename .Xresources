! General {{{ ------------------------------------------------------------------
URxvt.scrollBar:        false
!URxvt.perl-ext-common: tabbed

URxvt.saveLines:        16384

URxvt.externalBorder:   0
URxvt.internalBorder:   4

URxvt.cursorBlink:      true
! }}}
