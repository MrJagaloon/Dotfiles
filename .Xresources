! Font {{{ ---------------------------------------------------------------------
!URxvt.font:             xft:DeJavu Sans Mono:pixelsize=12
!URxvt.font:             -*-terminus-medium-*-normal-*-14-*-72-72-*-*-iso8859-*
URxvt.font:             -*-gohufont-medium-*-*-*-14-*

URxvt.letterSpace:      0

Xft.autohint:           0
Xft.dpi:                96
Xft.antialias:          false
Xft.rgba:               rgb
Xft.hinting:            true
Xft.hintingstyle:       hintslight
Xft.lcdfilter:          lcddefault
! }}}
! Rofi {{{ ---------------------------------------------------------------------
rofi.lines:             5 
rofi.eh:                2 
rofi.width:             100 
rofi.padding:           800 
rofi.bw:                0 
rofi.opacity:           85
rofi.color-window:      base00, base01, base0B, base0B, base0B
